{
    "name": "boat-game",
    "version": "1.0.0",
    "description": "A multiplayer 3D boat game with procedurally generated islands",
    "scripts": {
        "dev": "parcel serve index.html --open --hmr-port 1234 --watch-dir src",
        "build": "parcel build index.html --public-url ./ && cp -r public/* dist/",
        "start": "http-server dist -p $PORT"
    },
    "dependencies": {
        "@types/three": "^0.174.0",
        "firebase": "^11.4.0",
        "http-server": "^14.1.1",
        "nipplejs": "^0.10.2",
        "postprocessing": "^6.37.1",
        "socket.io-client": "^4.5.4",
        "three": "^0.174.0"
    },
    "devDependencies": {
        "@parcel/transformer-glsl": "^2.13.3",
        "buffer": "^6.0.3",
        "parcel": "^2.13.3"
    },
    "keywords": [
        "three.js",
        "game",
        "multiplayer",
        "socket.io",
        "3d",
        "boat"
    ],
    "parcel": {
        "staticFiles": {
            "staticPath": "assets",
            "staticOutPath": "assets"
        }
    },
    "author": "",
    "license": "MIT"
}